						],
						"policy": {
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "ingesta_capaplata_capaoro",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true,
							"parameters": {}
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"variables": {
					"Automatizacion": {
						"type": "String"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/ingesta_capabronce_capaplata')]",
				"[concat(variables('factoryId'), '/pipelines/ingesta_desdeinput_hastacapabronce')]",
				"[concat(variables('factoryId'), '/pipelines/ingesta_capaplata_capaoro')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Automatizacioncapas')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Job que corre el codigo de las 3 capas y el input",
				"annotations": [],
				"runtimeState": "Started",
