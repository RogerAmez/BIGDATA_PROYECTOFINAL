			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "ingesta de la capa bronce a la plata",
				"activities": [
					{
						"name": "Ingestadebronce_aplata",
						"type": "DatabricksNotebook",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/Users/roger_amez@hotmail.com/Ingesta_capabronceplata 2024-09-26 20:50:58"
						},
						"linkedServiceName": {
							"referenceName": "Cargadebronce_aplata",
							"type": "LinkedServiceReference"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/Cargadebronce_aplata')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/archivo_input_capabronce')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "conector_ADSL_Datafactory",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"Archivo_inputs": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@dataset().Archivo_inputs",
							"type": "Expression"
						},
						"folderPath": "input",
						"fileSystem": "contenedorproyectofinal"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Cod_Categoria",
						"type": "String"
					},
					{
						"name": "Nombre",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/conector_ADSL_Datafactory')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/archivo_inputd')]",
